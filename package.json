{
  "name": "vue3-starter-simple",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "description": "vue3-starter-simple",
  "author": "oiij",
  "license": "MIT",
  "homepage": "https://github.com/oiij/vue3-starter-simple",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "update:deps": "taze -w && pnpm i",
    "type:check": "vue-tsc --noEmit",
    "start": "node ./app.js",
    "preview": "vite preview",
    "cz": "czg",
    "commit": "git pull && git add -A && pnpm cz && git push",
    "pull": "git checkout -- . && git pull origin main",
    "release": "pnpm pull && pnpm build && pm2 restart 0",
    "awe": "pnpx are-we-esm",
    "nmi": "pnpx node-modules-inspector",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "npx simple-git-hooks"
  },
  "dependencies": {
    "@oiij/directives": "^0.0.9",
    "@oiij/naive-ui": "^0.0.45",
    "@oiij/use": "^0.0.21",
    "@oiij/utils": "^0.0.3",
    "@unhead/vue": "2.0.14",
    "@unocss/reset": "66.4.2",
    "@vueuse/core": "^13.6.0",
    "await-to-js": "^3.0.0",
    "axios": "^1.11.0",
    "colord": "^2.9.3",
    "mixte": "3.4.1",
    "nanoid": "^5.1.5",
    "notivue": "^2.4.5",
    "nprogress": "^0.2.0",
    "pinia": "^3.0.3",
    "pinia-plugin-persistedstate": "^4.5.0",
    "vue": "^3.5.18",
    "vue-hooks-plus": "2.4.0",
    "vue-i18n": "^11.1.11",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@iconify-json/mage": "^1.2.4",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@oiij/tsconfig": "^0.0.1",
    "@oiij/unocss-preset": "^0.0.1",
    "@plugin-web-update-notification/vite": "^2.0.1",
    "@shikijs/markdown-it": "^3.9.2",
    "@shikijs/twoslash": "^3.9.2",
    "@types/express": "^5.0.3",
    "@types/node": "^24.2.1",
