    "vitest": "^3.2.4",
    "vue-router-better-scroller": "^0.0.0",
    "vue-tsc": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged && pnpm type:check"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm lint:fix"
    ]
  }
}
