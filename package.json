    "@vitejs/plugin-vue-jsx": "^5.0.1",
    "@vitest/ui": "^3.2.4",
    "@vue-hooks-plus/resolvers": "^1.3.0",
    "@vue/test-utils": "^2.4.6",
    "commitlint": "^19.8.1",
    "crypto-js": "^4.2.0",
    "cz-git": "^1.12.0",
    "czg": "^1.12.0",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.0.1",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "jsdom": "^26.1.0",
    "less": "^4.4.0",
    "lint-staged": "^16.1.5",
    "markdown-it-link-attributes": "^4.0.1",
    "naive-ui": "^2.42.0",
    "postcss": "^8.5.6",
